{
  "name": "la-ritournelle",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run build-scripts --silent && react-static start",
    "stage": "npm run build-scripts --silent && react-static build --staging",
    "build": "npm run build-scripts --silent && react-static build",
    "serve": "serve dist -p 3000",
    "build-scripts": "tsc -p ./scripts",
    "deploy": "yarn build && yarn deploy-only",
    "deploy-only": "node ./scripts/Deploy",
    "deploy-php": "node ./scripts/DeployPhp",
    "deploy-failed": "node ./scripts/Fail",
    "before-ci": "yarn global add typescript"
  },
  "dependencies": {
    "@prettier/plugin-php": "^0.9.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.5",
    "global": "^4.3.2",
    "ical": "^0.5.0",
    "react": "^16.6.3",
    "react-calendar": "^2.17.5",
    "react-dom": "^16.6.3",
    "react-google-recaptcha": "^1.0.5",
    "react-hot-loader": "^4.8.0",
    "react-image-lightbox": "^5.1.0",
    "react-images": "^0.5.19",
    "react-static": "^5.9.12",
    "reactstrap": "^6.5.0",
    "showdown": "^1.9.0",
    "smoothscroll-polyfill": "^0.4.3",
    "twilio": "^3.25.0",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/reactstrap": "^6.4.3",
    "@types/showdown": "^1.7.5",
    "@types/webpack-env": "^1.13.5",
    "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
    "eslint-config-react-tools": "1.1.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "ftp-deploy": "^2.2.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "serve": "^10.1.1",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2"
  },
  "prettier": {
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "printWidth": 120,
    "tabWidth": 2
  }
}
